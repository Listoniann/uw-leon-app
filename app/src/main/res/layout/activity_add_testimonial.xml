<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AddTestimonial">

    <EditText
        android:id="@+id/schoolTes2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="UW Tacoma"
        android:inputType="text"
        tools:layout_editor_absoluteX="100dp"
        tools:layout_editor_absoluteY="258dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="School"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="274dp" />

    <EditText
        android:id="@+id/photoTes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Photo"
        android:inputType="text"
        tools:layout_editor_absoluteX="27dp"
        tools:layout_editor_absoluteY="568dp" />

    <EditText
        android:id="@+id/nameTes"
        android:layout_width="145dp"
        android:layout_height="45dp"
        android:ems="10"
        android:hint="First Name"
        android:inputType="text"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="121dp" />

    <EditText
        android:id="@+id/nameTes2"
        android:layout_width="145dp"
        android:layout_height="45dp"
        android:ems="10"
        android:hint="Last Name"
        android:inputType="text"
        tools:layout_editor_absoluteX="235dp"
        tools:layout_editor_absoluteY="121dp" />

    <EditText
        android:id="@+id/emailTes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="abc@uw.edu"
        android:inputType="text"
        tools:layout_editor_absoluteX="100dp"
        tools:layout_editor_absoluteY="195dp" />

    <EditText
        android:id="@+id/seasonTes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Summer 2023"
        android:inputType="text"
        tools:layout_editor_absoluteX="100dp"
        tools:layout_editor_absoluteY="320dp" />

    <EditText
        android:id="@+id/contentTes"
        android:layout_width="347dp"
        android:layout_height="96dp"
        android:ems="10"
        android:inputType="text"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="414dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Testimonial Request "
        android:textStyle="bold"
        tools:layout_editor_absoluteX="140dp"
        tools:layout_editor_absoluteY="40dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Name"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="89dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="UW Email"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="209dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Season"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="334dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Your Testimonial"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="384dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Add Photo"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="527dp" />

    <Button
        android:id="@+id/submitTes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Submit"
        tools:layout_editor_absoluteX="158dp"
        tools:layout_editor_absoluteY="650dp" />

</androidx.constraintlayout.widget.ConstraintLayout>